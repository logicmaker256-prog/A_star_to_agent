from heapq import heappush, heappop

# ===============================
# 全角マップ
# ===============================
MAP = [
"■■■Ｂ・・▲▲▲▲・・Ａ■■■",
"■■■■・・▲▲▲▲・・■■■■",
"■■■■・・▲▲▲▲・・■■■■",
"■■■■・・②＃＃＃・・■■■■",
"■■■■・・＃＃＃②・・■■■■",
"■■■■・・▲▲▲▲・・■■■■",
"・・・・・・▲▲▲▲・・・・・・",
"・・・・・・▲▲▲▲・・・・・・",
"▲＃①▲▲▲▲▲▲▲▲▲▲＃①▲",
"▲＃＃▲▲▲▲▲▲▲▲▲▲＃＃▲",
"▲＃＃▲▲▲▲▲▲▲▲▲▲＃＃▲",
"▲①＃▲▲▲▲▲▲▲▲▲▲①＃▲",
"・・・・・・▲▲▲▲・・・・・・",
"・・・・・・▲▲▲▲・・・・・・",
"■Ｃ■■・・②＃＃＃・・受■■■",
"■■■■・・＃＃＃②・・■■■■",
]

H, W = len(MAP), len(MAP[0])

# ===============================
# 定義
# ===============================
DIRS = {
    (-1, 0): "▲",
    (1, 0): "▼",
    (0, -1): "◀",
    (0, 1): "▶",
}

DIR_BIT = {
    (-1, 0): 0b0001,  # 北
    (0, -1): 0b0010,  # 西
    (1, 0): 0b0100,   # 南
    (0, 1): 0b1000,   # 東
}

BIT_TO_DIR = {v: k for k, v in DIR_BIT.items()}

ASTAR_OK = {"・", "＃", "①", "②", "Ａ", "Ｂ", "Ｃ", "受"}

# ===============================
# 回転
# ===============================
def rol(d): return ((d << 1) & 0b1111) or 0b0001
def ror(d): return (d >> 1) or 0b1000

# ===============================
# A*
# ===============================
def astar(start, goal):
    pq = []
    heappush(pq, (0, start))
    came = {start: None}
    cost = {start: 0}

    while pq:
        _, cur = heappop(pq)
        if cur == goal:
            break
        for dr, dc in DIR_BIT:
            nr, nc = cur[0] + dr, cur[1] + dc
            if 0 <= nr < H and 0 <= nc < W:
                if MAP[nr][nc] in ASTAR_OK:
                    ncost = cost[cur] + 1
                    if (nr, nc) not in cost or ncost < cost[(nr, nc)]:
                        cost[(nr, nc)] = ncost
                        h = abs(goal[0] - nr) + abs(goal[1] - nc)
                        heappush(pq, (ncost + h, (nr, nc)))
                        came[(nr, nc)] = cur

    path = []
    cur = goal
    while cur:
        path.append(cur)
        cur = came.get(cur)
    return path[::-1]

# ===============================
# Agent 実行
# ===============================
def simulate_agent(path, start_dir=0b0001):
    traj = []
    d = start_dir
    r, c = path[0]

    for i in range(len(path) - 1):
        nr, nc = path[i + 1]
        dr, dc = nr - r, nc - c
        need = DIR_BIT[(dr, dc)]

        if d == need:
            pass
        elif rol(d) == need:
            d = rol(d)
        elif ror(d) == need:
            d = ror(d)
        else:
            # 180度（2回転相当）
            d = rol(rol(d))

        r, c = nr, nc
        traj.append((r, c, d))

    return traj

# ===============================
# 可視化
# ===============================
def draw(astar_path, traj):
    m_astar = [list(r) for r in MAP]
    m_agent = [list(r) for r in MAP]

    for r, c in astar_path:
        if m_astar[r][c] in ASTAR_OK:
            m_astar[r][c] = "☆"

    for r, c, d in traj:
        dr, dc = BIT_TO_DIR[d]
        m_agent[r][c] = DIRS[(dr, dc)]

    print("=== A*ルート ===")
    for r in m_astar:
        print("".join(r))

    print("\n=== エージェント軌跡 ===")
    for r in m_agent:
        print("".join(r))

# ===============================
# 実行（受 → A → B → C → 受）
# ===============================
def find(ch):
    for r in range(H):
        for c in range(W):
            if MAP[r][c] == ch:
                return (r, c)

points = ["受", "Ａ", "Ｂ", "Ｃ", "受"]

full_path = []
for i in range(len(points) - 1):
    p = astar(find(points[i]), find(points[i + 1]))
    full_path += p if i == 0 else p[1:]

traj = simulate_agent(full_path)

draw(full_path, traj)